
<div class="contact_count">
  <h2>There are current <span class="contact_number"><%= @contacts.count %></span> contacts</h2>
</div>
  <ul class="contact_list">
    <%@contacts.each do |contact|%>
    <li class="contact_list"><%= contact.first_name -%> <%= contact.last_name -%> (<%= contact.email -%>)</li>
    <div class="contact_edit">
    <a class="contact_list" href="#">Edit</a> | <a class="contact_list" href="#">Delete</a>
    </div>
    <% end %>
  </ul>
